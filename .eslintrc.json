{
  "root": true,
  "env": {
    "browser": true
  },
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-console": 0,
    "import/extensions": 0,
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ]
  }
}